<section id="clearance" class="edit-user">
    <h2>
        <%= t(".title") %>
    </h2>

    <%= form_for @user do |form| %>

        <%= render partial: '/users/image_uploader', locals: { form: form } %>
        <%= render partial: '/users/form', locals: { form: form } %>

        <div class="submit-field">
            <%= form.submit %>
        </div>

    <% end %>
</section>